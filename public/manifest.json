{
    "manifest_version": 3,
    "name": "ChatGPT Chrome Extension",
    "version": "0.0.0.1",
    "description": "ChatGPT Chrome Extension by Carlos Garcia",
    "action": {
        "default_popup": "index.html",
        "default_title": "Open ChatGPT by Carlos Garcia",
        "default_icon": {
            "32": "icon32-high.png"
        }
    },
    "permissions": ["scripting", "storage"],
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}